
{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/sw.js",
      "destination": "/sw.js"
    },
    {
      "source": "/manifest-admin.json",
      "destination": "/manifest-admin.json"
    },
    {
      "source": "/admin",
      "destination": "/admin.html"
    },
    {
      "source": "/admin.html",
      "destination": "/admin.html"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/((?!admin|manifest-admin|sw.js|favicon|assets|.*\\.).*)",
      "destination": "/index.html"
    }
  ]
}
